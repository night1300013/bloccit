<h1 style="display:inline;">All Posts</h1>
<%= link_to "New Post", new_post_path, class: 'btn btn-success',\
 style: "margin:5px 5px 20px 5px;" %>

<% @posts.each do |post| %>
  <div class="media">
    <div class="media-body">
      <h4 class="media-heading">
        <%= link_to post.title, post %>
      </h4>
    </div>
  </div>
<% end %>
